{
  "name": "@victorekpo/codemod-utils",
  "version": "0.0.1-beta.2",
  "description": "Codemod utils",
  "keywords": [],
  "homepage": "https://github.com/victorekpo/codemod-utils#readme",
  "bugs": {
    "url": "https://github.com/victorekpo/codemod-utils/issues"
  },
  "repository": {
    "url": "git://github.com/victorekpo/codemod-utils.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "license": "ISC",
  "author": "Victor Ekpo",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run test && rm -rf dist && npm run type-check && rollup -c",
    "type-check": "tsc --noEmit",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules\" jest",
    "pack": "rm -f *.tgz && npm run build && npm pack && mv victorekpo-codemod-utils*.tgz codemod-utils.tgz",
    "pub": "npm run publish:github",
    "publish:github": "npm run build && npm publish --access public"
  },
  "dependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jscodeshift": "^0.12.0",
    "ast-types": "^0.16.1",
    "axios": "^1.8.1",
    "fast-glob": "^3.3.3",
    "jscodeshift": "^17.3.0",
    "recast": "^0.23.11",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.14",
    "jest": "^29.7.0",
    "rollup": "^4.37.0",
    "ts-jest": "^29.3.0",
    "tslib": "^2.8.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
